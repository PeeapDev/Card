{
  "version": 2,
  "buildCommand": "",
  "outputDirectory": "public",
  "functions": {
    "api/router.ts": {
      "maxDuration": 30,
      "memory": 1024
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/router"
    },
    {
      "source": "/checkout/pay/:sessionId",
      "destination": "/api/router"
    },
    {
      "source": "/assets/(.*)",
      "destination": "/assets/$1"
    },
    {
      "source": "/embed/(.*)",
      "destination": "/embed/$1"
    },
    {
      "source": "/cdn/(.*)",
      "destination": "/cdn/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
