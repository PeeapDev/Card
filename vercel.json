{
  "version": 2,
  "framework": "vite",
  "buildCommand": "cd apps/web && npm install --legacy-peer-deps && npm run build",
  "outputDirectory": "apps/web/dist",
  "installCommand": "echo 'skipping root install'",
  "git": {
    "deploymentEnabled": {
      "main": true
    }
  },
  "crons": [
    {
      "path": "/api/cron/keepalive",
      "schedule": "0 8 * * *"
    }
  ],
  "routes": [
    {
      "src": "/api/router/(.*)",
      "dest": "/api/router"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/router"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
