{
  "version": 2,
  "buildCommand": "",
  "outputDirectory": "public",
  "git": {
    "deploymentEnabled": {
      "main": true
    }
  },
  "functions": {
    "api/router.ts": {
      "maxDuration": 30,
      "memory": 1024
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/router"
    },
    {
      "src": "/checkout/pay/(.*)",
      "dest": "/api/router"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
