{
  "version": 2,
  "functions": {
    "api/router.ts": {
      "memory": 1024,
      "maxDuration": 30
    }
  },
  "rewrites": [
    { "source": "/", "destination": "/api/router" },
    { "source": "/(.*)", "destination": "/api/router" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "*" }
      ]
    }
  ]
}
