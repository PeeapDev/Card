{
  "version": 2,
  "name": "peeap-api",
  "functions": {
    "api/router.ts": {
      "maxDuration": 30,
      "memory": 1024
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/router"
    },
    {
      "source": "/(.*)",
      "destination": "/api/router"
    }
  ]
}
