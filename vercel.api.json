{
  "version": 2,
  "framework": null,
  "buildCommand": "",
  "installCommand": "npm install",
  "functions": {
    "api/router.ts": {
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/router",
      "headers": {
        "Cache-Control": "no-cache, no-store, must-revalidate",
        "X-Content-Type-Options": "nosniff",
        "Access-Control-Allow-Origin": "*"
      }
    }
  ]
}
